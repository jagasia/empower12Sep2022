<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" 
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="add1" class="com.empower.hrms.model.Address">
		<property name="dno" value="A451" />
		<property name="street" value="Main road" />
		<property name="city" value="Bengaluru" />
	</bean>

	<bean id="add2" class="com.empower.hrms.model.Address">
		<property name="dno" value="124" />
		<property name="street" value="MG Road" />
		<property name="city" value="Bengaluru" />
	</bean>


	<bean id="customer" class="com.empower.hrms.model.Customer" autowire="byType">
		<property name="id" value="123" />
		<property name="name" value="Rama" />
		
	</bean>
</beans>
